<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="textActionNameColor">@android:color/tertiary_text_light</color>
    <item name="cancel" type="string">Cancel</item>
    <item name="copy" type="string">Copy</item>
    <item name="cut" type="string">Cut</item>
    <item name="last" type="string">Last</item>
    <item name="more" translatable="false" type="string">More</item>
    <item name="next" type="string">Next</item>
    <item name="paste" type="string">Paste</item>
    <item name="replace" type="string">Replace</item>
    <item name="replaceAll" type="string">Replace All</item>
    <item name="replacement" type="string">Replacement</item>
    <item name="saveToProject" translatable="false" type="string">Project</item>
    <item formatted="true" name="selectAll" type="string">Select All</item>
    <item name="text_to_search" type="string">Text to search</item>
    <style name="ButtonBar.IconOnly.Left" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">8dp</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeBottomLeft">8dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
    </style>
    <style name="ButtonBar.IconOnly.Middle" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">0dp</item>
    </style>
    <style name="ButtonBar.IconOnly.Right" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">8dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">8dp</item>
    </style>
    <style name="HorizontalDivider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>
    <style name="OutlinedButton.IconOnly" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="iconPadding">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:minWidth">48dp</item>
        <item name="android:minHeight">48dp</item>
    </style>
    <style name="RoundOutlinedButton.IconOnly" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
        <item name="android:maxWidth">48dp</item>
        <item name="android:maxHeight">48dp</item>
    </style>
    <style name="TextButton.IconOnly" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="iconPadding">0dp</item>
        <item name="cornerFamily">cut</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:minWidth">48dp</item>
        <item name="android:minHeight">48dp</item>
    </style>
    <style name="VerticalDivider">
        <item name="android:layout_width">0.8dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>
    <declare-styleable name="editor">
        <attr format="string" name="text"/>
        <attr format="dimension" name="textSize"/>
        <attr format="dimension" name="dividerWidth"/>
        <attr format="boolean" name="lineNumberVisible"/>
        <attr format="boolean" name="autoCompleteEnabled"/>
        <attr format="boolean" name="symbolCompletionEnabled"/>
        <attr format="integer" name="cursorBlinkPeriod"/>
        <attr format="boolean" name="scrollbarsEnabled"/>
        <attr format="boolean" name="verticalScrollbarEnabled"/>
        <attr format="boolean" name="horizontalScrollbarEnabled"/>
    </declare-styleable>
</resources>